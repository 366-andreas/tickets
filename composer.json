{
    "name": "andreasnik/tickets",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Nikolaou",
            "email": "andreas.n@gtgroup.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "ext-json": "*",
        "illuminate/contracts": "^8.0|^9.0|^10",
        "illuminate/support": "^8.0|^9.0|^10",
        "laravel/tinker": "^2.7",
        "laravel/helpers": "^1.4",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": false,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "tinker": "vendor/bin/psysh"
    },
    "autoload": {
        "classmap": [
            "database/seeds/",
            "database/factories/",
            "database/migrations/"
        ],
        "psr-4": {
            "AndreasNik\\Ticket\\": "src/",
            "AndreasNik\\Ticket\\Database\\Factories\\": "database/factories/",
            "AndreasNik\\Ticket\\Database\\Seeders\\": "database/seeds"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "database/seeds/",
            "database/factories/",
            "database/migrations/",
            "tests"
        ],
        "psr-4": {
            "AndreasNik\\Ticket\\Tests\\": "tests/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    }
}
